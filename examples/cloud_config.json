{
    "app_name": "my-cloud-app",
    "environment": "development",
    "resources": {
        "compute": {
            "instance_type": "t2.micro",
            "count": 2
        },
        "storage": {
            "type": "S3",
            "size_gb": 100
        },
        "database": {
            "type": "RDS",
            "engine": "postgresql",
            "size": "db.t3.micro"
        }
    },
    "network": {
        "vpc_cidr": "10.0.0.0/16",
        "subnet_type": "public"
    }
}
